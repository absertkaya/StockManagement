@inherits ZXingScannerBase
@page "/zxing"
<h3>ZXingScanner</h3>

<div class="form-group">
    <div class="col-md-10">
        <input type="file" id="fileUpload" @ref="input" name="files" multiple />
    </div>
</div>
<div class="form-group">
    <div class="col-md-10">
        <input @onclick="Decode" type="submit" value="Decode" />
    </div>
</div>
<div class="form-group">
    <div class="col-md-10">
        <img src="" alt="" />
        <textarea id="codeField" @bind="res" rows="2" cols="20"></textarea>
    </div>
</div>


@code {

}
